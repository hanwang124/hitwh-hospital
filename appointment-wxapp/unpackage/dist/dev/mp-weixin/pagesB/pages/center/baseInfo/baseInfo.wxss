/**
 * 根据一些普片的css样式
 **/
.page-backgroud {
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
.visited-color {
  color: #7EC0EE !important;
}
.cut-long-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.tips-box {
  width: 100%;
  height: 100rpx;
  border: 1px solid #F0F0F0;
  background: #FFFEF5;
}
.tips-box .tips-inbox {
  width: 80%;
  height: 100%;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tips-box .tips-inbox .icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.tips-box .tips-inbox .tips-text {
  font-size: 17px;
  font-weight: 500;
  color: #A6A6A6;
}
.gray-border-box {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  border: 1px solid #BBBBBB;
  -webkit-border-radius: 8px;
          border-radius: 8px;
  padding: 10rpx;
  background: #FFFFFF;
}
.blue-column-border-box {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
}
.blue-column-border-box .blue-border {
  width: 5%;
  height: 50rpx;
  border-left: 6px solid #7EC0EE;
}
.blue-column-border-box .blue-text {
  width: 80%;
  font-size: 18px;
  font-weight: bold;
  color: #7EC0EE;
}
.list {
  width: 95%;
  margin: 0 auto;
  margin-top: 8px;
  margin-bottom: 5px;
  border: 1px;
  background-color: white;
  -webkit-box-shadow: 0 0 5px 5px #F6F6FA;
          box-shadow: 0 0 5px 5px #F6F6FA;
  -webkit-border-radius: 3%;
          border-radius: 3%;
}
.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 2% 3%;
  border-bottom: 1px solid #f3f3f3;
}
.textAvatarUrl {
  /* 头像处的样式 */
  font-size: 14px;
  line-height: 55px;
  font-weight: bold;
}
.textStyle {
  /* 其他的样式 */
  font-size: 14px;
  line-height: 33px;
  font-weight: bold;
}
.textData {
  font-size: 14px;
  line-height: 33px;
  color: #8F8F94;
}
.center_img {
  width: 55px;
  height: 55px;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  overflow: hidden;
  margin-right: 3%;
}
.center_img image {
  width: 100%;
  height: 100%;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  margin-top: 8rpx;
}
.turn_img {
  width: 13px;
  height: 13px;
  margin-left: 2px;
}
.titleStyle {
  color: #AAAAAA;
  border-bottom: 1px solid #f3f3f3;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  padding: 2% 3%;
}
.textimg {
  /* 箭头和文字 */
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  vertical-align: middle;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
/* 当用户没有添加自己的就诊卡时显示添加就诊卡 */
