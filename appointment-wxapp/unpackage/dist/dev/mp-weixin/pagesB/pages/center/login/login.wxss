/**
 * 根据一些普片的css样式
 **/
.page-backgroud {
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
.visited-color {
  color: #7EC0EE !important;
}
.cut-long-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.tips-box {
  width: 100%;
  height: 100rpx;
  border: 1px solid #F0F0F0;
  background: #FFFEF5;
}
.tips-box .tips-inbox {
  width: 80%;
  height: 100%;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tips-box .tips-inbox .icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.tips-box .tips-inbox .tips-text {
  font-size: 17px;
  font-weight: 500;
  color: #A6A6A6;
}
.gray-border-box {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  border: 1px solid #BBBBBB;
  -webkit-border-radius: 8px;
          border-radius: 8px;
  padding: 10rpx;
  background: #FFFFFF;
}
.blue-column-border-box {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
}
.blue-column-border-box .blue-border {
  width: 5%;
  height: 50rpx;
  border-left: 6px solid #7EC0EE;
}
.blue-column-border-box .blue-text {
  width: 80%;
  font-size: 18px;
  font-weight: bold;
  color: #7EC0EE;
}
/**
 * 关于表单的scss
 **/
.input-box, .password-box, .password-row-box, .input-inbox {
  width: 90%;
  height: 80rpx;
  margin: 0 auto;
  background: #FFFFFF;
  -webkit-border-radius: 4px;
          border-radius: 4px;
  border: 1px solid #BBBBBB;
}
.password-box, .password-row-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
}
.placeholder-class {
  color: #A6A6A6;
  font-size: 16px;
}
.button-box, .button {
  width: 90%;
  height: 80rpx;
  margin: 0 auto;
  background: #7EC0EE;
  line-height: 80rpx;
  color: #FFFFFF;
  font-weight: bold;
}
.forbiddenInput {
  color: #A6A6A6;
}
