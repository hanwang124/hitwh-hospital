<view class="body"><view class="top-box"><block wx:if="{{isAlreadyLogin}}"><view data-event-opts="{{[['tap',[['toBaseInfo']]]]}}" class="row-box" bindtap="__e"><image class="avatar-url-box" src="{{userBaseInfo.avatarUrl}}"></image><view class="name-card-box"><view class="name-text-box"><text class="name-text">{{userBaseInfo.name}}</text><image hidden="{{!(isGetTreatCard)}}" class="icon" src="{{gender===2?iconUrl+'female.png':iconUrl+'male.png'}}"></image></view><block wx:if="{{cardID!==''}}"><text class="card-text">{{"卡号："+cardID}}</text></block></view><image class="right-white-icon" src="/static/center/right-white.png"></image></view></block><block wx:else><view data-event-opts="{{[['tap',[['turnLogin']]]]}}" class="row-box" bindtap="__e"><image class="no-login-img" src="/static/center/unknownAvatar.png"></image><view class="no-login-text-box"><text class="login">点击注册/登录</text><text class="welcome">欢迎来到挂号系统</text></view></view></block></view><view class="account-acord-outbox"><view class="account-acord-inbox"><view data-event-opts="{{[['tap',[['toTreatRecord']]]]}}" class="left-right-half" bindtap="__e"><image class="icon" src="/static/center/treat-record.png"></image><text class="text">就诊记录</text></view><view data-event-opts="{{[['tap',[['toAppointRecord']]]]}}" class="left-right-half" bindtap="__e"><image class="icon" src="/static/center/appoint-record.png"></image><text class="text">挂号记录</text></view></view></view><view class="bottom-outbox"><view data-event-opts="{{[['tap',[['toCardList']]]]}}" class="option-inbox" bindtap="__e"><image class="icon" src="/static/center/card-list.png"></image><text class="text">我的家人</text></view><view data-event-opts="{{[['tap',[['toAwaitingQueue']]]]}}" class="option-inbox" bindtap="__e"><image class="icon" src="/static/center/paidui.png"></image><text class="text">候诊队列</text></view><view data-event-opts="{{[['tap',[['toCreditDetail']]]]}}" class="option-inbox" bindtap="__e"><image class="icon" src="/static/center/xinyong.png"></image><text class="text">信用详情</text></view><view data-event-opts="{{[['tap',[['toAboutUs']]]]}}" class="option-inbox" bindtap="__e"><image class="icon" src="/static/center/about-us.png"></image><text class="text">关于我们</text></view><block wx:if="{{isAlreadyLogin}}"><view data-event-opts="{{[['tap',[['outLogin']]]]}}" class="option-inbox" bindtap="__e"><image class="icon" src="/static/center/outLogin.png"></image><text class="text">退出登录</text></view></block></view></view>