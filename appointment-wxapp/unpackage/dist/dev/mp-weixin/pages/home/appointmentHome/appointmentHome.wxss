.page-backgroud {
  width: 100%;
  height: 100%;
  background: #F2F2F2;
}
.visited-color {
  color: #7EC0EE !important;
}
.cut-long-text {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.tips-box {
  width: 100%;
  height: 100rpx;
  border: 1px solid #F0F0F0;
  background: #FFFEF5;
}
.tips-box .tips-inbox {
  width: 80%;
  height: 100%;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.tips-box .tips-inbox .icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 30rpx;
}
.tips-box .tips-inbox .tips-text {
  font-size: 17px;
  font-weight: 500;
  color: #A6A6A6;
}
.gray-border-box {
  width: 90%;
  height: 100%;
  margin: 0 auto;
  border: 1px solid #BBBBBB;
  -webkit-border-radius: 8px;
          border-radius: 8px;
  padding: 10rpx;
  background: #FFFFFF;
}
.blue-column-border-box {
  width: 90%;
  height: 90rpx;
  margin: 0 auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
}
.blue-column-border-box .blue-border {
  width: 5%;
  height: 50rpx;
  border-left: 6px solid #7EC0EE;
}
.blue-column-border-box .blue-text {
  width: 80%;
  font-size: 18px;
  font-weight: bold;
  color: #7EC0EE;
}
.text-position {
  width: 60%;
  height: 100%;
  margin: 0 auto;
  text-align: left;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.VerticalNav.nav {
  width: 200rpx;
  white-space: initial;
}
.VerticalNav.nav .cu-item {
  width: 100%;
  text-align: center;
  background-color: #fff;
  margin: 0;
  border: none;
  height: 50px;
  position: relative;
}
.VerticalNav.nav .cu-item.cur {
  background-color: #f1f1f1;
}
.VerticalNav.nav .cu-item.cur::after {
  content: "";
  width: 8rpx;
  height: 30rpx;
  -webkit-border-radius: 10rpx 0 0 10rpx;
          border-radius: 10rpx 0 0 10rpx;
  position: absolute;
  background-color: currentColor;
  top: 0;
  right: 0rpx;
  bottom: 0;
  margin: auto;
}
.VerticalBox {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.VerticalMain {
  background-color: #f1f1f1;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}

